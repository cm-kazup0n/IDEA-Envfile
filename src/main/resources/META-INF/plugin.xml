<idea-plugin>
    <id>com.github.kazup0n.idea_envfile_plugin</id>
    <name>idea_envfile</name>
    <vendor>Kazuhiro Sasaki</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>
    <extensions defaultExtensionNs="com.intellij">
        <runConfigurationExtension implementation="com.github.kazup0n.envfile.AddDotEnvVariablesConfigurationExtension"></runConfigurationExtension>
    </extensions>
</idea-plugin>
